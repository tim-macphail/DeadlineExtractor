    "react-dom": "^19",
    "react-pdf-highlighter": "^8.0.0-rc.0",
    "vite": "^7.1.3",
    "vite-plugin-dts": "^4.5.4"
  },
  "devDependencies": {
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/bun": "latest"
  }
}
